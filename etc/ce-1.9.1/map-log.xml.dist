<map xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="../map.xsd">
    <source>
        <document_rules>
            <rename>
                <document>log_visitor</document>
                <to>customer_visitor</to>
            </rename>
            <log_changes>
                <document key="visitor_id">log_visitor</document>
            </log_changes>
        </document_rules>
        <field_rules>
            <ignore>
                <field>log_visitor.first_visit_at</field>
            </ignore>
            <ignore>
                <field>log_visitor.last_url_id</field>
            </ignore>
            <ignore>
                <field>log_visitor.store_id</field>
            </ignore>
        </field_rules>
    </source>
    <destination>
        <document_rules>
            <ignore>
                <document>log_visitor</document>
            </ignore>
        </document_rules>
        <field_rules>
            <ignore>
                <field>customer_visitor.customer_id</field>
            </ignore>
        </field_rules>
    </destination>
</map>
