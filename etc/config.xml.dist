<config xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="config.xsd">
    <steps>
        <step>\Migration\Step\Integrity</step>
    </steps>
    <source>
        <database host="localhost" name="magento1" user="root"/>
    </source>
    <destination>
        <database host="localhost" name="magento2" user="root"/>
    </destination>
    <options>
        <map_file>etc/map.xml</map_file>
        <settings_map_file>settings-map-file.xml</settings_map_file>
        <bulk_size>100</bulk_size>
        <source_prefix>prefix</source_prefix>
        <dest_prefix>prefix<dest_prefix>
    </options>
</config>
