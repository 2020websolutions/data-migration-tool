<?xml version="1.0" encoding="UTF-8"?>
<map xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="../../../../../etc/map.xsd">
    <source>
        <document_rules>
            <ignore>
                <document>catalog_category_entity_url_key</document>
            </ignore>
            <ignore>
                <document>catalog_product_entity_url_key</document>
            </ignore>
            <ignore>
                <document>catalog_category_product</document>
            </ignore>
            <ignore>
                <document>catalog_product_website</document>
            </ignore>
            <ignore>
                <document>core_config_data</document>
            </ignore>
            <ignore>
                <document>core_store</document>
            </ignore>
            <ignore>
                <document>enterprise_url_rewrite</document>
            </ignore>
            <ignore>
                <document>enterprise_url_rewrite_redirect</document>
            </ignore>
            <ignore>
                <document>url_rewrite_m2*</document>
            </ignore>
            <ignore>
                <document>source_table_ignored</document>
            </ignore>
            <ignore>
                <document>table_ignored</document>
            </ignore>
            <ignore>
                <document>source_table_*</document>
            </ignore>
            <rename>
                <document>source_table_renamed</document>
                <to>dest_table_renamed</to>
            </rename>
            <rename>
                <document>table_with_data</document>
                <to>table_without_data</to>
            </rename>
        </document_rules>
        <field_rules>
            <ignore>
                <field>source_table.ignored_field</field>
            </ignore>
            <ignore>
                <field>common_table.source_field_ignored</field>
            </ignore>
            <move>
                <field>common_table.field1</field>
                <to>common_table.field1_moved</to>
            </move>
            <move>
                <field>source_table_renamed.moved_field</field>
                <to>dest_table_renamed.moved_field</to>
            </move>
        </field_rules>
    </source>
    <destination>
        <document_rules>
            <ignore>
                <document>dest_table_ignored</document>
            </ignore>
            <ignore>
                <document>dest_table_*</document>
            </ignore>
            <ignore>
                <document>catalog_category_entity_varchar</document>
            </ignore>
            <ignore>
                <document>catalog_product_entity_varchar</document>
            </ignore>
            <ignore>
                <document>url_rewrite</document>
            </ignore>
        </document_rules>
        <field_rules>
            <ignore>
                <field>common_table.dest_field_ignored</field>
            </ignore>
            <transform>
                <field>common_table.common_field</field>
                <handler class="\Migration\Handler\SetValue">
                    <param name="value" value="123" />
                </handler>
            </transform>
        </field_rules>
    </destination>
</map>
